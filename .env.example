# Authorization token required from clients
AUTH_TOKEN=your_auth_token

# Wex API credentials
WEX_API_URL=https://api.wex.com/merchant/log
WEX_USERNAME=your_username
WEX_PASSWORD=your_password
MERCHANT_CODE=*

# Test mode (true/false). When true, skips calling Wex and uses dummy response
TEST_MODE=false
# Downstream webhook URL to receive parsed responses
WEBHOOK_URL=https://your.webhook/endpoint
